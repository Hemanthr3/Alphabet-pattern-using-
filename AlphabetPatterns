14.import java.util.Scanner;

public class N {

	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("Enter the dimension of the square");
		int n = scan.nextInt();
		for (int i=0;i<=n-1;i++)
		{
			for (int j=0;j<=n-1;j++)
			{
				if(j==0||i==j||j==n-1)
				{
					System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}

}

